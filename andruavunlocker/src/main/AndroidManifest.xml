<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rcmobilestuff.security">



    <!-- -->
    <!-- Registration  https://stackoverflow.com/questions/11770794/how-to-set-permissions-in-broadcast-sender-and-receiver-in-android -->
    <!-- https://stackoverflow.com/questions/11770794/how-to-set-permissions-in-broadcast-sender-and-receiver-in-android -->
    <permission android:name="andro2.jf.mypermission"
        android:label="only_me"
        ></permission>

    <uses-permission android:name="com.android.vending.BILLING" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:enabled="true"

        >
        <activity android:name=".MainActivity"
            android:launchMode="singleTask"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <receiver android:name=".AndruavRegistration"
            android:enabled="true"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="rcmobilestuff.security.ANDRUAV_REGISTRATION" />
            </intent-filter>
        </receiver>


    </application>

</manifest>